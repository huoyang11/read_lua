define plua_Tval
p print_value($arg0,$arg1)
end

define plua_Ttable
printf "%s",print_table($arg0,hvalue($arg1))
end

define plua_table
printf "%s",print_table($arg0,$arg1)
end

define plua_str
p (char *)(((TString *)$arg0)->contents)
end

define plua_code
printf "%s",PrintCode($arg0,$arg1)
end

define plua_lscode
plua_code $arg0->L $arg0->fs.f
end

define plua_token
p print_token($arg0->t.token)
end